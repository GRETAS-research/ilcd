    <LCIAMethodInformation>
        <dataSetInformation>
            <common:UUID>992c8e8d-769a-4930-9b0f-4fa323250738</common:UUID>
            <common:name xml:lang="en">ILCD2011; Eutrophication freshwater; endpoint; PDF; ReCiPe</common:name>
            <methodology>ILCD2011</methodology>
            <methodology>ReCiPe2008</methodology>
            <impactCategory>Aquatic Eutrophication</impactCategory>
            <areaOfProtection>Natural environment</areaOfProtection>
            <impactIndicator>Potentially Disappeared Fraction of species (PDF) expressing the loss of species caused by aquatic eutrophication.</impactIndicator>
            <common:generalComment xml:lang="en">Limited European validity. Only impact on freshwater considered.</common:generalComment>
            <referenceToExternalDocumentation type="source data set" refObjectId="e58cd71d-881d-4b21-8331-f9880e0e53d0" uri="../sources/e58cd71d-881d-4b21-8331-f9880e0e53d0.xml">
                <common:shortDescription xml:lang="en">Struijs et al. (2008)</common:shortDescription>
            </referenceToExternalDocumentation>
        </dataSetInformation>
        <quantitativeReference>
            <referenceQuantity type="flow property data set" refObjectId="9643f3bf-731d-4bef-9a71-b8390472171a" uri="../flowproperties/9643f3bf-731d-4bef-9a71-b8390472171a.xml">
                <common:shortDescription xml:lang="en">Potentially Disappeared Number of freshwater species * time</common:shortDescription>
            </referenceQuantity>
        </quantitativeReference>
        <time>
            <referenceYear xml:lang="en">time independent</referenceYear>
            <duration xml:lang="en">indefinite</duration>
        </time>
        <geography>
            <interventionLocation>RER</interventionLocation>
            <impactLocation>NL</impactLocation>
            <geographicalRepresentativenessDescription xml:lang="en">Applicability to Europe, though only impacts on Dutch ecosystems are considered at endpoint.</geographicalRepresentativenessDescription>
        </geography>
        <impactModel>
            <modelName>CARMEN model for waterborne emissions and EUTREND for airborne emissions.</modelName>
            <modelDescription xml:lang="en">The exposure factors for water-borne emissions are calculated using the CARMEN model. Predicted P concentrations are linked to ecosystem damage using database correlating P concentrations with macro fauna species diversity in Dutch ecosystems.</modelDescription>
            <referenceToModelSource type="source data set" refObjectId="e58cd71d-881d-4b21-8331-f9880e0e53d0" uri="../sources/e58cd71d-881d-4b21-8331-f9880e0e53d0.xml">
                <common:shortDescription xml:lang="en">Struijs et al. (2008)</common:shortDescription>
            </referenceToModelSource>
            <consideredMechanisms xml:lang="en">Inclusion of important removal mechanisms for P (limiting nutrient) in the fate model.</consideredMechanisms>
            <referenceToMethodologyFlowChart type="source data set" refObjectId="3ad3809a-3eec-4ac9-9052-66b0e679b992" uri="../sources/3ad3809a-3eec-4ac9-9052-66b0e679b992.xml">
                <common:shortDescription xml:lang="en">Eutrophication_figure</common:shortDescription>
            </referenceToMethodologyFlowChart>
        </impactModel>
    </LCIAMethodInformation>
